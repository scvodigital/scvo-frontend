  <script type="text/javascript" src="/assets/js/scotlandforeurope/root.js"></script>

  {{#compare request.url.hostname "scotlandforeurope.local" ~}}
    <script type="text/javascript" src="/assets/scotlandforeurope/main.js"></script>
  {{else}}
    <script type="text/javascript" src="https://storage.googleapis.com/scvo-assets/scotlandforeurope/main-{{@root.context.metaData.assetsVersion}}.js"></script>
  {{/compare ~}}

  {{{layoutParts.foot}}}
</body>
</html>
