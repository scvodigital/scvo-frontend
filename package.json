        "@angular/forms": "^2.4.9",
        "@angular/http": "^2.4.9",
        "@angular/platform-browser": "^2.4.9",
        "@angular/platform-browser-dynamic": "^2.4.9",
        "@angular/router": "^3.4.9",
        "@ngtools/webpack": "^1.2.12",
        "@types/elasticsearch": "^5.0.13",
        "@types/headroom": "^0.7.30",
        "@types/jquery": "^2.0.40",
        "@types/lodash": "^4.14.55",
