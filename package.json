{
    "name": "scvo-frontend",
    "version": "0.3.0",
    "description": "SCVO Website",
    "scripts": {
        "ng": "ng",
        "start": "ng serve -p 2000",
        "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
        "test": "ng test",
        "build": "rm -rf ./dist && ng build --prod --env=prod",
        "deploy": "npm run build && npm run firebase-deploy",
        "firebase-deploy": "firebase deploy --project digital-participation-charter",
        "pree2e": "webdriver-manager update --standalone false --gecko false",
        "e2e": "protractor"
    },
    "contributors": [
        "Calum Mac√ôisdean <calum.macuisdean@scvo.org.uk>"
    ],
    "license": "MIT",
