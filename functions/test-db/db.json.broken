ine-block;\n    }\n    li.pull-right:before,\n    li:first-child:before {\n        content: \"\" !important;\n        font-weight: 800;\n        margin-right: 0 !important;\n        width: 0 !important;\n    }\n    li.pull-right a::after {\n        content: \"\";\n    }\n    li:not(:last-child) {\n        margin-right: $spacing-unit/4;\n    }\n}\n.spacer {\n    margin: 0 0 $spacing-unit*1.5;\n}\n.bank-accounts.comparison {\n    .mdc-layout-grid__cell--span-3-desktop {\n        a:after {\n            content: \"\" !important;\n            display: none;\n        }\n    }\n}\n\n.post-info {\n    font-size: 0.9rem;\n    color: #333;\n    .post-info-item {\n        display: inline;\n        margin-right: 0.5rem;\n    }\n    .post-info-item:last-child {\n        margin-right: 0;\n    }\n    .post-info-author {\n        a:hover {\n            text-decoration: none;\n        }\n        a:hover .underline {\n            text-decoration: underline;\n        }\n        img {\n            width: 1.6rem;\n            border-radius: 50%;\n            margin-right: $spacing-unit/2;\n            vertical-align: text-bottom;\n            float: left;\n            margin-top: -0.15rem;\n        }\n    }\n    .post-info-share,\n    .post-info-categories {\n        .fa {\n            color: var(--mdc-theme-primary-dark);\n        }\n        .inline-list {\n            li {\n                display: inline-block;\n                font-size: 90%;\n                margin: 0 0 0.25rem 0.1rem;\n                a {\n                    background: var(--mdc-theme-primary-dark);\n                    padding: 0.05rem 0.4rem 0.1rem;\n                    border-radius: $spacing-unit;\n                    color: #fff;\n                }\n                a:hover {\n                    background: var(--mdc-theme-primary);\n                    text-decoration: none;\n                }\n            }\n        }\n    }\n}\n.full .post-info {\n    margin: $spacing-unit/2 0 $spacing-unit*1.5;\n    bor