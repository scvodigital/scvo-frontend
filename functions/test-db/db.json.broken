            "sass": "@import url(\"//cdnjs.cloudflare.com/ajax/libs/normalize/7.0.0/normalize.min.css\");\n@import url(\"//fonts.googleapis.com/css?family=Open+Sans:300,400,500,700,800\");\n@import url(\"//fonts.googleapis.com/css?family=Oleo+Script:700\");\n\n@font-face {\n    font-family: 'goodhq';\n    src: url(\"assets/fonts/goodhq.eot\");\n    src: url(\"assets/fonts/goodhq.eot?#iefix\") format(\"embedded-opentype\"), url(\"assets/fonts/goodhq.woff\") format(\"woff\"), url(\"assets/fonts/goodhq.ttf\") format(\"truetype\"), url(\"assets/fonts/goodhq.svg#goodhq\") format(\"svg\");\n    font-weight: normal;\n    font-style: normal;\n}\n\n$base-font-family: \"Open Sans\";\n$base-font-weight: 400;\n$base-font-color: #222;\n$base-font-size: 1.5rem;\n$base-font-size-lg: 1.6rem;\n$heading-font-weight-light: 500;\n$heading-font-weight-heavy: 600;\n$button-font-weight: 600;\n\n$spacing-unit: 2rem;\n$border-width: 0.1rem;\n$avatar-size: 3.3rem;\n\n:root{\n    --mdc-theme-primary: #4033E9;\n    --mdc-theme-primary-light: #E0DEFD;\n    --mdc-theme-primary-dark: #0B0558;\n    --mdc-theme-secondary: #9224E7;\n    --mdc-theme-secondary-light: #EEDBFD;\n    --mdc-theme-secondary-dark: #310255;\n    --mdc-theme-background: #ECEFF1;\n    --mdc-site-background: #263238;\n    --mdc-layout-grid-margin-desktop: 0;\n    --mdc-layout-grid-margin-tablet: 0;\n\n    // $brand-lighter: #E0DEFD;\n    // $brand-light: #938BF4;\n    // $brand-primary: #4033E9;\n    // $brand-dark: #160AAD;\n    // $brand-darker: #0B0558;\n    //\n    // $complementary-lighter: #FFF7DB;\n    // $complementary-light: #FFE27F;\n    // $complementary-primary: #FFCA18;\n    // $complementary-dark: #F8BF00;\n    // $complementary-darker: #7F6200;\n    //\n    // $organisation-lighter: #EEDBFD;\n    // $organisation-light: #C281F4;\n    // $organisation-primary: #9224E7;\n    // $organisation-dark: #6005A6;\n    // $organisation-darker: #310255;\n    //\n    // $individual-lighter: #DBEFFC;\n    // $individual-light: #80C5F2;\n    // $individual-primary: #2298E5;\n    // $individual-dark: #04619E;\n    // $individual-darker: #023251;\n}\n\nhtml {\n    overflow-y: scroll;\n    height: 100%;\n    font-size: 0.96rem;\n    line-height: 1.45;\n}\nbody {\n    background-color: var(--mdc-site-background, #263238);\n    min-height: 100%;\n    padding: 0;\n    margin: 0;\n}\nmain {\n    background-color: white;\n}\n.container-fluid {\n    max-width: 1280px;\n    width: 95%;\n    margin: 0 auto;\n    overflow: auto;\n    padding: $spacing-unit/2;\n}\n\n.brand-logo {\n    margin-left: 0;\n    img {\n        max-height: $spacing-unit*3.5;\n    }\n}\n\n.mdc-tab,\n.mdc-button,\n.mdc-typography,\n.mdc-card__subtitle,\n.mdc-textfield__input,\n.mdc-card__title--large,\n.mdc-card__supporting-text,\n.mdc-typography--subheading2,\n.mdc-list {\n    font-family: 'Open Sans', Helvetica, sans-serif;\n}\n\n.mdc-typography {\n    background-color: var(--mdc-site-background, #263238);\n    padding: 0;\n    margin: 0;\n    box-sizing: border-box;\n}\na {\n    font-weight: 600;\n    color: var(--mdc-theme-primary-dark, #006064);\n    text-decoration: none;\n    text-decoration-skip: ink;\n}\na:hover {\n    text-decoration: underline;\n    color: var(--mdc-theme-primary, #00838F);\n}\n.mdc-card__actions a:hover,\n.mdc-button a:hover,\n.mdc-toolbar a:hover {\n    text-decoration: none;\n}\n\n.home-splash {\n    height: auto;\n    // background: linear-gradient(rgba(0, 0, 0, 0.5), rgba(0, 0, 0, 0.7)), url(\"/images/home/ghq-splash-2.jpg\") no-repeat center center;\n    // background-size: cover;\n    padding-top: $spacing-unit;\n    padding-bottom: $spacing-unit;\n\n    h1, h2, h3, p, small, ul, li {\n        color: white;\n        text-align: center !important;\n    }\n\n    h2 {\n        line-height: 3rem;\n        margin: 0;\n        font-size: 3rem !important;\n        font-weight: 600;\n        letter-spacing: -0.04rem;\n        margin: 0 0 ($spacing-unit/3);\n    }\n\n    h3 {\n        line-height: 3rem;\n        margin: 0 0 $spacing-unit;\n        font-size: 2.45rem !important;\n        font-weight: $base-font-weight;\n        letter-spacing: -0.04rem;\n    }\n\n    h1.splash-subtitle {\n        margin-bottom: $spacing-unit*2;\n        line-height: 0.5rem;\n    }\n\n    li {\n        margin-bottom: $spacing-unit/2;\n    }\n\n    li.notes {\n        margin-top: $spacing-unit/3;\n        margin-bottom: 0;\n    }\n\n    p {\n        margin-top: $spacing-unit/2;\n        font-size: $base-font-size;\n    }\n\n    .btn {\n        border: $border-width solid white;\n    }\n\n    .btn-default {\n        background-color: transparent;\n        color: white !important;\n    }\n\n    .btn-default:hover {\n        background-color: rgba(255, 255, 255, 0.33);\n    }\n\n    .hello {\n        color: white;\n        border-radius: $spacing-unit/6;\n        padding: $spacing-unit/2;\n        background-color: rgba(0, 0, 0, 0.67);\n\n        p {\n            margin-bottom: 0;\n        }\n    }\n\n    .home-spacer {\n        height: 250px;\n        display: block;\n    }\n\n    form {\n        margin: 0;\n    }\n\n    .home-brand {\n        background-image: url(\"assets/images/goodhq-logo.svg\");\n        background-repeat: no-repeat;\n        background-size: contain;\n        background-position: 50%;\n        height: 95px;\n        display: block;\n    }\n\n    .home-intro {\n        margin-bottom: $spacing-unit/2;\n    }\n\n    .home-search {\n        margin-bottom: $spacing-unit;\n    }\n\n    // .home-buttons {\n    //     margin: 0 0 ($spacing-unit*6);\n    // }\n    .charityawards {\n        margin-top: $spacing-unit*2;\n        .btn {\n            height: $spacing-unit*3;\n            line-height: 5rem;\n            padding-right: 50px;\n            .icon-vote {\n                float: left;\n                margin-top: 4px;\n            }\n        }\n    }\n\n    .splash-nav {\n        padding-bottom: $spacing-unit*4;\n    }\n}\n\n.user-canvas {\n    padding-top: 0;\n    background-image: linear-gradient(rgba(0, 0, 0, 0.1), rgba(0, 0, 0, 0.6)), url(\"assets/images/ghq-organisation-default-banner-1280x221.jpg\");\n    background-size: cover;\n    padding-bottom: $spacing-unit*1.1;\n\n    .cta {\n        margin-top: -40px;\n        .btn {\n            height: $spacing-unit*2;\n            line-height: 2.9rem;\n            font-size: 1.6rem;\n            font-weight: $button-font-weight;\n            margin: 0;\n            vertical-align: middle;\n            padding: $spacing-unit/4 1.5rem $spacing-unit/4 $spacing-unit/2;\n            .glyphicon {\n                top: 7px !important;\n                position: relative !important;\n                float: left;\n            }\n        }\n    }\n}\n\n.user-canvas:before {\n    content: \"\";\n    display: block;\n    padding-bottom: 18%;\n}\n\n\nfooter a {\n    text-decoration: underline;\n}\nfooter a:hover {\n    color: white;\n}\nfooter .mdc-list .mdc-list-item a:hover {\n    text-decoration: underline !important;\n}\n// Footer ======================================================================\n\n.footer {\n    background: rgb(55,71,79);\n    background: -moz-linear-gradient(top, rgba(55,71,79,1) 0%, rgba(38,50,56,1) 100%);\n    background: -webkit-linear-gradient(top, rgba(55,71,79,1) 0%,rgba(38,50,56,1) 100%);\n    background: linear-gradient(to bottom, rgba(55,71,79,1) 0%,rgba(38,50,56,1) 100%);\n    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#37474f', endColorstr='#263238',GradientType=0 );\n    color: rgba(255, 255, 255, 0.84);\n    padding-top: $spacing-unit*2.5;\n    padding-bottom: $spacing-unit*2;\n    clear: both;\n    .scvo-logo {\n        margin-top: $spacing-unit;\n    }\n    ul {\n        margin: 0;\n        padding: 0;\n    }\n    li {\n        text-align: right;\n        list-style-type: none;\n    }\n    li,\n    p {\n        font-size: 1.65rem;\n        color: white;\n    }\n    a {\n        text-decoration: underline;\n        text-decoration-skip: ink;\n        color: white;\n        line-height: 1.3;\n    }\n\n    .copyright {\n        margin-top: $spacing-unit;\n        line-height: 1.5rem;\n    }\n\n    p.footer-feedback {\n        margin-top: $spacing-unit/2;\n    }\n    p.footer-feedback,\n    .footer-copyright p {\n        small {\n            font-size: 88%;\n        }\n    }\n}\n\n.mdc-form-field {\n    font-size: 1rem;\n}\n.mdc-select,\n.mdc-textfield {\n    width: 100%;\n    margin: 0 0 $spacing-unit/2;\n}\n\n.mdc-toolbar {\n    height: 60px;\n    .mdc-toolbar__icon,\n    .mdc-toolbar__row,\n    .mdc-form-field,\n    .mdc-toolbar__section {\n        padding: 0;\n    }\n    .mdc-toolbar__row {\n        .container-fluid {\n            display: -webkit-box;\n            display: -ms-flexbox;\n            display: flex;\n            position: relative;\n            -webkit-box-sizing: border-box;\n            box-sizing: border-box;\n            padding: 0;\n            .mdc-toolbar__title {\n                padding: $spacing-unit/4 0 0;\n                margin-left: 0;\n            }\n            .mdc-textfield {\n                margin: 0 -1.7rem 0 0;\n                max-height: $spacing-unit*2.2;\n                height: auto;\n                .mdc-textfield__input,\n                .mdc-textfield__input:focus,\n                .mdc-textfield__input:hover {\n                    color: rgba(255,255,255,.87);\n                    border: $spacing-unit/10 solid rgba(255,255,255,.75);\n                    padding: $spacing-unit/2;\n                }\n                ::-webkit-input-placeholder {\n                    color: rgba(255, 255, 255, 0.6);\n                }\n                ::-moz-placeholder {\n                    color: rgba(255, 255, 255, 0.6);\n                }\n                :-ms-input-placeholder {\n                    color: rgba(255, 255, 255, 0.6);\n                }\n                :-moz-placeholder {\n                    color: rgba(255, 255, 255, 0.6);\n                }\n            }\n            .mdc-toolbar__icon {\n                height: $spacing-unit*2;\n                z-index: 100;\n            }\n            .mdc-tab-bar {\n                overflow: hidden;\n                text-transform: none;\n                height: $spacing-unit*2.5;\n                margin: 0;\n                .mdc-tab {\n                    color: rgba(255, 255, 255, 0.95);\n                    font-size: 1rem;\n                    font-weight: 600;\n                    padding: 0 $spacing-unit;\n                }\n                .mdc-tab:first-child {\n                    padding-left: 0;\n                }\n                .mdc-tab::after {\n                    display: none;\n                }\n            }\n            #upper-tab-bar {\n                margin-right: $spacing-unit;\n                .mdc-tab {\n                    min-width: auto;\n                    font-weight: 600;\n                }\n            }\n        }\n        a.mdc-toolbar__icon:hover {\n            color: white !important;\n        }\n        .mdc-toolbar__icon--menu {\n            background: transparent;\n            color: white;\n            border: none;\n            box-shadow: none;\n            cursor: pointer;\n            visibility: hidden;\n            width: 0;\n            .fa-bars {\n                font-size: 1.7rem;\n            }\n        }\n        .mdc-toolbar__icon--menu:focus {\n            outline: none;\n        }\n        .mdc-toolbar__icon:first-of-type {\n            padding-left: 0;\n        }\n        .mdc-toolbar__icon:last-of-type {\n            padding-right: 0;\n        }\n        .mdc-toolbar__section.toolbar-search {\n            vertical-align: middle;\n            .fa-search {\n                font-size: 1.3rem;\n            }\n        }\n        .mdc-toolbar__section.toolbar-menu {\n            .mdc-tab {\n                min-width: auto;\n            }\n        }\n    }\n}\n.mdc-toolbar-fixed-adjust {\n    margin-top: 60px;\n}\n\n.mdc-temporary-drawer {\n    .mdc-temporary-drawer__header-content {\n        padding: $spacing-unit;\n    }\n    .mdc-temporary-drawer__header:before {\n        padding-top: 24%;\n    }\n    .mdc-list {\n        .mdc-list-item {\n            padding: 0 $spacing-unit;\n        }\n        .mdc-list-item__start-detail {\n            display: none;\n        }\n    }\n}\n",
