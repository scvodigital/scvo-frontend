ht: $spacing-unit/2; }\n\n.horizontal-list-details {\n    display: flex;\n    padding: 0 0 0;\n    margin-top: 8px;\n    margin-bottom: $spacing-unit/2;\n\n    li {\n        min-width: 15%;\n        height: auto;\n        align-self: baseline;\n    }\n\n    li:not(:last-child){\n        margin-right: 5px;\n    }\n}\n\nfooter {\n    color: white;\n    background-color: var(--mdc-footer-primary);\n    border-top: 0.6rem solid var(--mdc-footer-primary-light);\n    .footer-main {\n        padding: $spacing-unit*2.5 0 $spacing-unit*2;\n    }\n    a {\n        color: white;\n        text-decoration: underline;\n    }\n    a:hover {\n        color: white;\n    }\n    .mdc-list .mdc-list-item a:hover {\n        text-decoration: underline !important;\n    }\n    .brand-logo {\n        img {\n            margin-bottom: $spacing-unit;\n        }\n    }\n    .mdc-layout-grid {\n        padding: 0;\n        margin-bottom: 0;\n        p {\n            margin: 0 0 0.8rem;\n        }\n    }\n    .mdc-list {\n        padding: 0;\n        .mdc-list-item {\n            height: 1.5rem;\n            a {\n                text-decoration: none;\n            }\n        }\n    }\n    .footer-copyright {\n        background-color: var(--mdc-footer-primary-dark);\n        font-size: 0.85rem;\n        .row {\n            position: relative;\n        }\n        .logo {\n            position: absolute;\n            bottom: 8px;\n            right: 0.75rem;\n        }\n        p.copyright {\n            margin-bottom: 0;\n        }\n    }\n}\n\n@media screen and (max-width: 991px) {\n    .mdc-toolbar {\n        .container-fluid .mdc-toolbar__icon--menu {\n            visibility: visible;\n            width: auto;\n        }\n        #upper-tab-bar,\n        .mdc-toolbar__row.toolbar-nav {\n            display: none;\n        }\n    }\n    .latest-content {\n        .mdc-layout-grid {\n            width: 100%;\n        }\n    }\n    .mdc-toolbar.mdc-toolbar--fixed {\n        padding-top: 0;\n    }