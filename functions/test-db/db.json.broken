            "sass": "@import url(https://cdnjs.cloudflare.com/ajax/libs/normalize/7.0.0/normalize.min.css);\n\n:root{\n    --mdc-theme-primary: #58a934;\n    --mdc-theme-primary-light: #8bdb63;\n    --mdc-theme-primary-dark: #217900;\n    --mdc-theme-secondary: #594743;\n    --mdc-theme-secondary-light: #86726e;\n    --mdc-theme-secondary-dark: #2f201c;\n    --mdc-theme-background: #ededed;\n}\n\n.hero {\n    display: -webkit-box;\n    display: -ms-flexbox;\n    display: flex;\n    -webkit-box-orient: horizontal;\n    -webkit-box-direction: normal;\n    -ms-flex-flow: row nowrap;\n    flex-flow: row nowrap;\n    -webkit-box-align: center;\n    -ms-flex-align: center;\n    align-items: center;\n    -webkit-box-pack: center;\n    -ms-flex-pack: center;\n    justify-content: center;\n    /* height: 360px; */\n    min-height: 360px;\n    background-color: rgba(0, 0, 0, 0.05);\n\n    .mdc-card{\n        background-color:#fff;\n    }\n}\n\n.demo-body {\n    padding: 0;\n    margin: 0;\n    box-sizing: border-box;\n}\n\n.container-fluid {\n    max-width: 1280px;\n    width: 95%;\n    margin: 0 auto;\n    overflow: auto;\n    padding: 15px;\n}\n\n.action-buttons-right {\n    flex-direction: row-reverse;\n    border-top: 1px solid #dddddd;\n}\n\n.horizontal-list-details {\n    display: flex;\n    padding: 0 0 0;\n    margin-top: 8px;\n    margin-bottom: 16px;\n    \n    li {\n        min-width: 15%;\n        height: auto;\n        align-self: baseline;\n    }\n\n    li:not(:last-child){\n        margin-right: 5px;\n    }\n}\n",
            "template": "<!doctype html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"utf-8\">\n    <title>Scvo</title>\n    <base href=\"/\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\">\n    <link rel=\"icon\" type=\"image/x-icon\" href=\"favicon.ico\">\n    <link type=\"text/css\" rel=\"stylesheet\" href=\"https://fonts.googleapis.com/icon?family=Material+Icons\">\n    <link type=\"text/css\" rel=\"stylesheet\" href=\"https://unpkg.com/material-components-web@latest/dist/material-components-web.min.css\">\n    <link type=\"text/css\" rel=\"stylesheet\" href=\"https://storage.googleapis.com/scvo-net.appspot.com/goodmoves-material-colours.css\">\n    <style type=\"text/css\">\n        {{{this.css}}}\n    </style>\n</head>\n<body>\n    <scvo-site>\n        <div class=\"mdc-typography demo-body\" style=\"\">\n            <div class=\"mdc-toolbar mdc-toolbar--fixed\">\n                <div class=\"mdc-toolbar__row\">\n                    <section class=\"mdc-toolbar__section mdc-toolbar__section--align-start\">\n                        <button class=\"demo-menu material-icons mdc-toolbar__icon--menu\">menu</button>\n                        <span class=\"mdc-toolbar__title catalog-title\">{{title}}</span>\n                    </section>\n                    <section class=\"mdc-toolbar__section mdc-toolbar__section--align-end\">\n                        <div class=\"mdc-form-field mdc-form-field--align-end\">\n                            <div class=\"mdc-textfield\">\n                                <input type=\"text\" class=\"mdc-textfield__input\" placeholder=\"Search\" id=\"search\">\n                            </div>\n                        </div>\n                    </section>\n                </div>\n            </div>\n\n            <aside class=\"mdc-temporary-drawer\" style=\"\">\n                <nav class=\"mdc-temporary-drawer__drawer\">\n                    <header class=\"mdc-temporary-drawer__header\">\n                        <div class=\"mdc-temporary-drawer__header-content mdc-theme--primary-bg mdc-theme--text-primary-on-primary\">\n                            {{title}} \n                        </div>\n                    </header>\n                    <nav class=\"mdc-temporary-drawer__content mdc-list-group\">\n                        <div id=\"icon-with-text-demo\" class=\"mdc-list\">\n                            <a class=\"mdc-list-item\" href=\"/\" data-mdc-tabindex-handled=\"true\" tabindex=\"-1\">\n                                <i class=\"material-icons mdc-list-item__start-detail\" aria-hidden=\"true\">home</i>Home\n                            </a>\n                            <a class=\"mdc-list-item\" href=\"/search\" data-mdc-tabindex-handled=\"true\" tabindex=\"-1\">\n                                <i class=\"material-icons mdc-list-item__start-detail\" aria-hidden=\"true\">work</i>Vacancies\n                            </a>\n                            <a class=\"mdc-list-item\" href=\"/organisations\" data-mdc-tabindex-handled=\"true\" tabindex=\"-1\">\n                                <i class=\"material-icons mdc-list-item__start-detail\" aria-hidden=\"true\">business</i>Organisations\n                            </a>\n                        </div>\n                    </nav>\n                </nav>\n            </aside>\n\n            <main class=\"mdc-toolbar-fixed-adjust\">\n                <div class=\"page-content\">\n                    <router-outlet>{{{routeHtml}}}</router-outlet>        \n                </div>\n            </main>\n        </div>\n    </scvo-site>\n    <script type=\"text/javascript\" src=\"https://unpkg.com/material-components-web@latest/dist/material-components-web.min.js\"></script>\n    <script type=\"text/javascript\">\n        var drawerEl = document.querySelector('.mdc-temporary-drawer');\n        var MDCTemporaryDrawer = mdc.drawer.MDCTemporaryDrawer;\n        var drawer = new MDCTemporaryDrawer(drawerEl);\n        do